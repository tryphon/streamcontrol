<%= title_tag t(".title") %>

<table class="events">
  <% for event in @events %>

  <tr>
    <td><%= event.severity_image_tag %></td>
    <td><%= event.localized_created_at %></td>
    <td><%= event.user_message %></td>
    <td><%= event.stream_link %></td>
  </tr>

  <% end %>
</table>

<%= will_paginate @events %>
